@model Library.Models.Entity.tblemployee
@{
    ViewBag.Title = "FetchEmployee";
    Layout = "~/Views/Shared/Layout.cshtml";
}

@using (Html.BeginForm("UpdateEmployee", "Employee", FormMethod.Post))
{
    <div style="margin-bottom:15px"></div>
    <div class=" form-group">
        @Html.HiddenFor(e => e.id)
        @Html.HiddenFor(e => e.id, new { @class = "form-control" })
    </div>
    <div>
        @*@Html.LabelFor(c=>c.name)*@
        <label>Employee</label>
        @Html.TextBoxFor(e => e.employee, new { @class = "form-control" })
    </div>
    <br />
    <button class="btn btn-warning">Update</button>
}

