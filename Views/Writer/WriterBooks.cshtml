@using Library.Models.Entity
@model  List<tblbook>

@{
    ViewBag.Title = "WriterBooks";
    Layout = "~/Views/Shared/Layout.cshtml";
}
<br />
<h2> @ViewBag.w</h2>
<br />
<table class="table table-striped">
    <tr>
        <th>ID</th>
        <th>BOOK NAME</th>
        <th>CATEGORY</th>
        <th>PUBİCATION HOME</th>
        <th>PUBİCATION YEAR</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.id</td>
            <td>@x.name</td>
            <td>@x.category</td>
            <td>@x.publicationhome</td>
            <td>@x.publicationyear</td>
        </tr>
    }
</table>

